pipeline {
    agent any
    stages {
        stage('Build and run docker image') {
            steps {
                sh 'sudo docker pull mallika1109/millioniumfalcon:latest'
                sh 'sudo docker run -d -p 8082:80 mallika1109/millioniumfalcon:latest'
            } 
        }


        stage('testing') {
            steps {
                sh 'curl -I 34.235.139.150:8082'
            }
        }

    
    }
}
